    "express": "^4.19.2",
    "nodemon": "^3.1.4",
    "prisma": "^5.18.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "@prisma/client": "^5.18.0",
    "pg": "^8.12.0",
    "uuid": "^10.0.0"
  }
}
